<div class="list_element">

<h1>Login</h1>
 
<% form_for @user_session do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :login %><br />
    <%= f.text_field :login %><br />
  </p>
  <p>
    <%= f.label :password %><br />
    <%= f.password_field :password %><br />
  </p>
  <p>
    <%= f.check_box :remember_me %><%= f.label :remember_me %><br />
  </p>
  <br />
  <%= f.submit "Login" %>
<% end %>
</div>